    "suggest": {
        "phpdocumentor/reflection-docblock": "*",
        "symfony/asset": "^5.0",
        "symfony/validator": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andras Prim",
            "email": "tzunghaor+github@gmail.com"
        }
