{
    "name": "tzunghaor/settings-bundle",
    "description": "Database persisted settings",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": ">=2.7.0",
        "symfony/cache": "^5.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/form": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/routing": "^5.0",
